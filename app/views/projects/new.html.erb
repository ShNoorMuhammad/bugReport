<div class="container">
<h1>New Project</h1>
<%= form_for(@project, :html => { class: "form-horizontal", role: "form" }) do |f| %>
<div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "name" %>
  </div>
  <br/>
  
  
  <div class="row">
    <div class="col-md-offset-2 col-md-4">
    <p>Developer</p>
  <%= f.collection_select(:user_ids,User.where(usertype: "Developer"), :id, :name,{}, {:multiple => true}) %>
    
    <p>QA's</p>
  <%= f.collection_select(:user_ids,User.where(usertype: "QA"), :id, :name,{}, {:multiple => true})%>

       </div>
  </div>


  <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
          <%= f.submit class: "btn btn-primary " %>
        </div>
      </div>
<% end %>
</div>